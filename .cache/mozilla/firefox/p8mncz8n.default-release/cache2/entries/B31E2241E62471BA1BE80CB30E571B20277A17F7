<script>
	function fsearchbox_submit(f)
	{
        // í•œê¸€ì ê²€ìƒ‰ ê°€ëŠ¥ í‚¤ì›Œë“œ
        if(f.stx.value == 'ì¶•' ||  f.stx.value == 'ì¤„' || f.stx.value == 'íœ ') {

        } else {
            if (f.stx.value.length < 2) {
                alert("ê²€ìƒ‰ì–´ëŠ” ë‘ê¸€ì ì´ìƒ ì…ë ¥í•˜ì‹­ì‹œì˜¤.");
                f.stx.select();
                f.stx.focus();
                return false;
            }
        }


		// ê²€ìƒ‰ì— ë§ì€ ë¶€í•˜ê°€ ê±¸ë¦¬ëŠ” ê²½ìš° ì´ ì£¼ì„ì„ ì œê±°í•˜ì„¸ìš”.
		var cnt = 0;
		for (var i=0; i<f.stx.value.length; i++) {
			if (f.stx.value.charAt(i) == ' ')
				cnt++;
		}

		if (cnt > 2) {
			alert("ë¹ ë¥¸ ê²€ìƒ‰ì„ ìœ„í•˜ì—¬ ê²€ìƒ‰ì–´ì— ê³µë°±ì€ 2ê°œë§Œ ì…ë ¥í•  ìˆ˜ ìˆìŠµë‹ˆë‹¤.");
			f.stx.select();
			f.stx.focus();
			return false;
		}

		return true;
	}
</script>



