function pop_pop(URL, OPT, WIDTH, HEIGHT, NAME){
	
	var popwidth = WIDTH;
	var popheight = HEIGHT;
	var winT = ((screen.height-popheight)/2)-50;
	var winR = (screen.width-popwidth)/2;

	var spec = 'toolbar=no,';
	spec += 'scrollbars=yes,';
	spec += 'status=no,';
	spec += 'height='+popheight+',';
	spec += 'top='+winT+',';
	spec += 'left='+winR+',';
	spec += 'width='+popwidth;
	
	var popup = window.open(URL+"?"+OPT, NAME, spec);
	popup.focus();
}

