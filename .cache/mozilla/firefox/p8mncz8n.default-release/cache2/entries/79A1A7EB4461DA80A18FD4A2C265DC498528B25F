/**
 * мљ°нЋёлІ€нё м°Ѕ
 **/
var win_zip = function(frm_name, frm_zip, frm_addr1, frm_addr2, frm_addr3, frm_jibeon) {
    if(typeof daum === 'undefined'){
        alert("л‹¤мќЊ мљ°нЋёлІ€нё postcode.v2.js нЊЊмќјмќґ лЎњл“њлђм§Ђ м•Љм•мЉµл‹€л‹¤.");
        return false;
    }

    var zip_case = 1;   //0мќґл©ґ л €мќґм–ґ, 1мќґл©ґ нЋмќґм§Ђм—ђ лЃјм›Њ л„Јкё°, 2мќґл©ґ мѓ€м°Ѕ

    var complete_fn = function(data){
        // нЊќм—…м—ђм„њ кІЂмѓ‰кІ°кіј н•­лЄ©мќ„ нЃґл¦­н–€мќ„л•Њ м‹¤н–‰н•  мЅ”л“њлҐј мћ‘м„±н•лЉ” л¶Ђл¶„.

        // к°Ѓ мЈјм†Њмќ л…ём¶њ к·њм№™м—ђ л”°лќј мЈјм†ЊлҐј мЎ°н•©н•њл‹¤.
        // л‚ґл ¤м¤лЉ” ліЂм€к°Ђ к°’мќґ м—†лЉ” кІЅмљ°м—” кіµл°±('')к°’мќ„ к°Ђм§ЂлЇЂлЎњ, мќґлҐј м°ёкі н•м—¬ л¶„кё° н•њл‹¤.
        var fullAddr = ''; // мµњмў… мЈјм†Њ ліЂм€
        var extraAddr = ''; // мЎ°н•©н• мЈјм†Њ ліЂм€

        // м‚¬мљ©мћђк°Ђ м„ нѓќн•њ мЈјм†Њ нѓЂмћ…м—ђ л”°лќј н•ґл‹№ мЈјм†Њ к°’мќ„ к°Ђм ёмЁл‹¤.
        if (data.userSelectedType === 'R') { // м‚¬мљ©мћђк°Ђ лЏ„лЎњлЄ… мЈјм†ЊлҐј м„ нѓќн–€мќ„ кІЅмљ°
            fullAddr = data.roadAddress;

        } else { // м‚¬мљ©мћђк°Ђ м§ЂлІ€ мЈјм†ЊлҐј м„ нѓќн–€мќ„ кІЅмљ°(J)
            fullAddr = data.jibunAddress;
        }

        // м‚¬мљ©мћђк°Ђ м„ нѓќн•њ мЈјм†Њк°Ђ лЏ„лЎњлЄ… нѓЂмћ…мќјл•Њ мЎ°н•©н•њл‹¤.
        if(data.userSelectedType === 'R'){
            //лІ•м •лЏ™лЄ…мќґ мћ€мќ„ кІЅмљ° м¶”к°Ђн•њл‹¤.
            if(data.bname !== ''){
                extraAddr += data.bname;
            }
            // к±ґл¬јлЄ…мќґ мћ€мќ„ кІЅмљ° м¶”к°Ђн•њл‹¤.
            if(data.buildingName !== ''){
                extraAddr += (extraAddr !== '' ? ', ' + data.buildingName : data.buildingName);
            }
            // мЎ°н•©н•мЈјм†Њмќ мњ л¬ґм—ђ л”°лќј м–‘мЄЅм—ђ кґ„нёлҐј м¶”к°Ђн•м—¬ мµњмў… мЈјм†ЊлҐј л§Њл“ л‹¤.
            extraAddr = (extraAddr !== '' ? ' ('+ extraAddr +')' : '');
        }

        // мљ°нЋёлІ€нём™Ђ мЈјм†Њ м •ліґлҐј н•ґл‹№ н•„л“њм—ђ л„Јкі , м»¤м„њлҐј мѓЃм„ёмЈјм†Њ н•„л“њлЎњ мќґлЏ™н•њл‹¤.
        var of = document[frm_name];

        of[frm_zip].value = data.zonecode;

        of[frm_addr1].value = fullAddr;
        of[frm_addr3].value = extraAddr;

        if(of[frm_jibeon] !== undefined){
            of[frm_jibeon].value = data.userSelectedType;
        }

        of[frm_addr2].focus();
    };

    switch(zip_case) {
        case 1 :    //iframeмќ„ мќґмљ©н•м—¬ нЋмќґм§Ђм—ђ лЃјм›Њ л„Јкё°
            var daum_pape_id = 'daum_juso_page'+frm_zip,
                element_wrap = document.getElementById(daum_pape_id),
                currentScroll = Math.max(document.body.scrollTop, document.documentElement.scrollTop);
            if (element_wrap == null) {
                element_wrap = document.createElement("div");
                element_wrap.setAttribute("id", daum_pape_id);
                element_wrap.style.cssText = 'display:none;border:1px solid;left:0;width:100%;height:300px;margin:5px 0;position:relative;-webkit-overflow-scrolling:touch;';
                element_wrap.innerHTML = '<img src="//i1.daumcdn.net/localimg/localimages/07/postcode/320/close.png" id="btnFoldWrap" style="cursor:pointer;position:absolute;right:0px;top:-21px;z-index:1" class="close_daum_juso" alt="м ‘кё° лІ„нЉј">';
                jQuery('form[name="'+frm_name+'"]').find('input[name="'+frm_addr1+'"]').before(element_wrap);
                jQuery("#"+daum_pape_id).off("click", ".close_daum_juso").on("click", ".close_daum_juso", function(e){
                    e.preventDefault();
                    jQuery(this).parent().hide();
                });
            }

            new daum.Postcode({
                oncomplete: function(data) {
                    complete_fn(data);
                    // iframeмќ„ л„ЈмќЂ elementлҐј м•€ліґмќґкІЊ н•њл‹¤.
                    element_wrap.style.display = 'none';
                    // мљ°нЋёлІ€нё м°ѕкё° н™”л©ґмќґ ліґмќґкё° мќґм „мњјлЎњ scroll мњ„м№лҐј лђлЏЊл¦°л‹¤.
                    document.body.scrollTop = currentScroll;
                },
                // мљ°нЋёлІ€нё м°ѕкё° н™”л©ґ нЃ¬кё°к°Ђ мЎ°м •лђм—€мќ„л•Њ м‹¤н–‰н•  мЅ”л“њлҐј мћ‘м„±н•лЉ” л¶Ђл¶„.
                // iframeмќ„ л„ЈмќЂ elementмќ л†’мќґк°’мќ„ мЎ°м •н•њл‹¤.
                onresize : function(size) {
                    element_wrap.style.height = size.height + "px";
                },
                width : '100%',
                height : '100%'
            }).embed(element_wrap);

            // iframeмќ„ л„ЈмќЂ elementлҐј ліґмќґкІЊ н•њл‹¤.
            element_wrap.style.display = 'block';
            break;
        case 2 :    //мѓ€м°ЅмњјлЎњ лќ„мљ°кё°
            new daum.Postcode({
                oncomplete: function(data) {
                    complete_fn(data);
                }
            }).open();
            break;
        default :   //iframeмќ„ мќґмљ©н•м—¬ л €мќґм–ґ лќ„мљ°кё°
            var rayer_id = 'daum_juso_rayer'+frm_zip,
                element_layer = document.getElementById(rayer_id);
            if (element_layer == null) {
                element_layer = document.createElement("div");
                element_layer.setAttribute("id", rayer_id);
                element_layer.style.cssText = 'display:none;border:5px solid;position:fixed;width:300px;height:460px;left:50%;margin-left:-155px;top:50%;margin-top:-235px;overflow:hidden;-webkit-overflow-scrolling:touch;z-index:10000';
                element_layer.innerHTML = '<img src="//i1.daumcdn.net/localimg/localimages/07/postcode/320/close.png" id="btnCloseLayer" style="cursor:pointer;position:absolute;right:-3px;top:-3px;z-index:1" class="close_daum_juso" alt="л‹«кё° лІ„нЉј">';
                document.body.appendChild(element_layer);
                jQuery("#"+rayer_id).off("click", ".close_daum_juso").on("click", ".close_daum_juso", function(e){
                    e.preventDefault();
                    jQuery(this).parent().hide();
                });
            }

            new daum.Postcode({
                oncomplete: function(data) {
                    complete_fn(data);
                    // iframeмќ„ л„ЈмќЂ elementлҐј м•€ліґмќґкІЊ н•њл‹¤.
                    element_layer.style.display = 'none';
                },
                width : '100%',
                height : '100%'
            }).embed(element_layer);

            // iframeмќ„ л„ЈмќЂ elementлҐј ліґмќґкІЊ н•њл‹¤.
            element_layer.style.display = 'block';
    }
}

