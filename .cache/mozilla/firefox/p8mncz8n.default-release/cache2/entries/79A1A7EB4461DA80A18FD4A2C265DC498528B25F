/**
 * мЉ¤нЃ¬л¦°л¦¬лЌ” лЇём‚¬мљ©мћђлҐј мњ„н•њ мЉ¤нЃ¬л¦ЅнЉё - м§Ђмљґм•„л№  2013-04-22
 * alt к°’л§Њ к°–лЉ” к·ёлћн”Ѕ л§ЃнЃ¬м—ђ л§€мљ°мЉ¤м¤лІ„ м‹њ title к°’ л¶Ђм—¬, л§€мљ°мЉ¤м•„м›ѓ м‹њ title к°’ м њк±°
 **/
$(function() {
    $('a img').mouseover(function() {
        $a_img_title = $(this).attr('alt');
        $(this).attr('title', $a_img_title);
    }).mouseout(function() {
        $(this).attr('title', '');
    });
});

